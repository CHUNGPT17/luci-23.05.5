# For full documentation of mwan3 configuration:
# https://openwrt.org/docs/guide-user/network/wan/multiwan/mwan3#mwan3_configuration


config globals 'globals'
	option mmx_mask '0x3F00'

config rule 'youtubev4'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option ipset 'youtubev4'
	option use_policy 'balanced'

config rule 'youtubev6'
	option family 'ipv6'
	option proto 'all'
	option sticky '0'
	option ipset 'youtubev6'
	option use_policy 'balanced'

config rule 'https'
	option sticky '1'
	option dest_port '443'
	option proto 'tcp'
	option use_policy 'balanced'

config rule 'default_rule_v4'
	option dest_ip '0.0.0.0/0'
	option use_policy 'balanced'
	option family 'ipv4'

config rule 'default_rule_v6'
	option dest_ip '::/0'
	option use_policy 'balanced'
	option family 'ipv6'


